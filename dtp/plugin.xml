<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
		 id="com.dynatrace.diagnostics.amazon.account.plugin"
         point="com.dynatrace.diagnostics.InstallerContent">
      <metainfo
            installer_type="resourcepack"
            name="Amazon Account Monitor Plugin and Dashboard">
		 
		 <resource
               resource="dashboards/Amazon Account Overview.dashboard.xml"
               resource_type="dashboard">
         </resource>
		 
		 <resource
               resource="dashboards/Amazon Cost by Usage.dashboard.xml"
               resource_type="dashboard">
         </resource>

         <resource
               resource="profiles/AmazonAccount.profile.xml"
               resource_type="systemProfile"
			   overwrite="false">
         </resource>

         <resource
               resource="plugin/com.dynatrace.diagnostics.plugin.AmazonAccountMonitor.jar"
               resource_type="userPlugin">
         </resource>
      </metainfo>
   </extension>
</plugin>
